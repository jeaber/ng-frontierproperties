@import 'jeet'
#photos
	col(1)
	margin auto 2%
	width 96%
	max-width none
	margin-bottom 1em
	.gallery-item
		col(1/3, cycle: 3, gutter: 1em)
		overflow hidden
		margin-top 1em
		border-radius 3%
		img
			width 100%
		+below(3)
			col(1/2, cycle: 2, uncycle: 3, gutter: 1em)
#photos.hidden
	.gallery-item:nth-child(n+4)
		display: none
	+below(3)
		.gallery-item:nth-child(n+3)
			display none
	
	
	
			